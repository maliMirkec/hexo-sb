{
  "wbConfig": {
    "src": "helpers.source/config.js.src/sw.wb",
    "dest": "helpers.source/config.js.src/sw.js"
  },
  "swConfig": {
    "globDirectory": "helpers.dist/",
    "globPatterns": [
      "**/*.{html,webmanifest,css,eot,svg,ttf,woff,woff2,png,js,ico,jpg}"
    ],
    "globIgnores": [
      "docs/**/*",
      "gfx/cover/**/*"
    ],
    "swDest": "helpers.dist/sw.js",
    "swSrc": "helpers.source/sw/sw.js"
  },
  "eslintConfig": {
    "configFile": "helpers.proot/.eslintrc.json",
    "fix": true,
    "quiet": true
  },
  "includeConfig": {
    "hardFail": true,
    "includePaths": [
      "helpers.proot/node_modules"
    ]
  },
  "babelConfig": {
    "presets": ["@babel/env"]
  },
  "standardConfig": {
    "breakOnError": false,
    "showRuleNames": true,
    "quiet": true,
    "showFilePath": true
  },
  "renameConfig": {
    "suffix": ".min"
  }
}
