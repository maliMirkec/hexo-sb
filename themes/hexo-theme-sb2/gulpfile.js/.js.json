{
  "swConfig": {
    "globDirectory": "../../public/",
    "globPatterns": [
      "**/*.{html,webmanifest,css,js,eot,svg,ttf,woff,woff2,png,ico,jpg}"
    ],
    "globIgnores": [
      "bookmarks/**/*",
      "archives/**/*",
      "tags/**/*",
      "favicon/*.{png,jpg,svg}",
      "*.critical.css"
    ],
    "swDest": "helpers.dist/sw.js",
    "swSrc": "helpers.source/sw/sw.js"
  },
  "eslintConfig": {
    "configFile": "helpers.proot/.eslintrc.json",
    "fix": true,
    "quiet": true
  },
  "includeConfig": {
    "hardFail": true,
    "includePaths": [
      "helpers.proot/node_modules"
    ]
  },
  "babelConfig": {
    "presets": ["@babel/env"]
  },
  "standardConfig": {
    "breakOnError": false,
    "showRuleNames": true,
    "quiet": true,
    "showFilePath": true
  },
  "renameConfig": {
    "suffix": ".min"
  }
}
