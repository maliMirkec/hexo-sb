{
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 6
  },
  "rules": {
    "no-console": "off",
    "no-restricted-globals": "off"
  },
  "globals": {
    "requestAnimationFrame": true,
    "sessionStorage": true,
    "self": true
  },
  "env": {
    "browser": true,
    "es6": true,
    "amd": true,
    "commonjs": true,
    "serviceworker": true
  }
}
