.block__grid.block__grid--alpha
  .block__item
    include intro-profile
  .block__item.block__item--beta.profile
    .profile__image
      .profile__avatar
        +cloudinary("https://res.cloudinary.com/starbist/image/upload/w_220/v1576584716/profile-2019-circle-small_sp95uf.png", "", "Headshot of myself, Silvestar Bistrović.", "", "false")
        +cloudinary("https://res.cloudinary.com/starbist/image/upload/w_220/v1582730484/avatar_gc1swk.jpg", "", "Avatar of myself, Silvestar Bistrović.", "", "false")
    .profile__content
      .profile__inner
        h1.profile__title Silvestar Bistrović
        p.profile__tagline Web developer since 2012
        ul.profile__list.profile__list--alpha.profile__tags
          li.profile__tag Frontend
          li.profile__tag User Interface
          li.profile__tag HTML5
          li.profile__tag CSS3
          li.profile__tag JavaScript
          li.profile__tag JAMstack
          li.profile__tag WordPress
        ul.profile__positions
          li.profile__position
            h3.h4 Fearless web engineer
          li.profile__position
            h3.h4 CSS developer
          li.profile__position
            h3.h4 JAMstack enthusiast
          li.profile__position
            h3.h4 WordPress theme developer
        ul.profile__list.profile__list--beta.profile__links
          li.profile__link
            a.profile__href(href="/portfolio/") Portfolio
          li.profile__link
            a.profile__href(href="/about-me/") About
          li.profile__link
            a.profile__href(href="/contact/") Contact
