if (theme && theme.widgets && theme.widgets.subnav)
  nav.subnav
    ul.subnav__list
      each kcontact, vcontact in theme.widgets.subnav
        li.subnav__item
          if (kcontact == "/" && theme && theme.site.logo_footer)
            a.subnav__link(href=kcontact)
              if (page && page.offline)
                img(src='data:image/svg+xml;base64,', alt=vcontact, width="30", height="30")
              else
                img(src=theme.site.logo_footer, alt=vcontact)
          else if (kcontact.indexOf("mailto") === -1 && kcontact.indexOf("http") !== -1)
            a.subnav__link(href=kcontact, rel="me noopener", target="_blank")= vcontact
          else
            a.subnav__link(href=kcontact)= vcontact
