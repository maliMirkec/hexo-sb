{% if theme and theme.site and theme.site.language %}
  {% assign lang = theme.site.language %}
{% else if config and config.language %}
  {% assign lang = config.language %}
{% else %}
  {% assign lang = "en" %}
{% endif %}

<!DOCTYPE html>
<html lang="{{ lang }}">
  {% include _partials/head %}
  <body>
    {% include _partials/header %}
    <main id="main">
      {% block content %}
        {% if sections %}
          {% for section in sections %}
            {% include _partials/sections %}
          {% endfor %}
        {% endif %}
        {% if content != blank and content != '' %}
          {% include _partials/content %}
        {% endif %}
        {% if sections2 %}
          {% for section in sections2 %}
            {% include _partials/sections %}
          {% endfor %}
        {% endif %}
      {% endblock %}
    </main>
    {% include _partials/footer %}
    {% include _partials/_assets/style %}
    <script src="/js/cldnry.js"></script>
    <h1>Test</h1>
    <script src="/js/cldnry.min.js" async></script>
    <script src="/js/cldnry2.min.js" async></script>
    <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
    {% include _partials/_assets/scripts %}
  </body>
</html>




